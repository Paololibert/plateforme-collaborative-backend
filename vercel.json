{
  "version": 2,
  "buildCommand": "prisma generate && prisma migrate deploy && npm run build",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "installCommand": "npm install"
}
{
  "version": 2,
  "buildCommand": "cd .. && npx prisma generate && npx prisma migrate deploy && npm run build",
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "installCommand": "npm install",
  "env": {
    "PRISMA_SCHEMA_PATH": "./backend/prisma/schema.prisma"
  }
}